@model List<StronglyTypedViewsExample.Models.Employee>

<html>
    <head>
        <title>Index2</title>
    </head>
    <body>
        <h4>Strongly Typed Views with Collection</h4>
        <table border="1">
            <tr>
                <th>Emp ID</th>
                <th>Emp Name</th>
                <th>Salary</th>
            </tr>
            @if (Model.Count > 4)
            {
                foreach (var emp in Model)
                {
                    <tr>
                        <td>@emp.EmpID</td>
                        <td>@emp.EmpName</td>
                        <td>@emp.Salary</td>
                    </tr>
                }
            }
        </table>
    </body>
</html>
